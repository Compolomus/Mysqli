{
  "name": "compolomus/collection",
  "description": "Collection",
  "license": "MIT",
  "keywords": [
    "collection"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Koenig",
      "email": "compolom@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Compolomus\\Collection\\": "src/"
    }
  },
  "require": {
    "php": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "zendframework/zend-coding-standard": "^1.0"
  },
  "suggest": {
    "ext-xdebug": "^2.6.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test-coverage": "phpunit --coverage-clover clover.xml"
  },
  "minimum-stability": "stable"
}
